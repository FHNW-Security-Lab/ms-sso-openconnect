<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.github.ms-sso-openconnect-ui.helper</string>

    <key>ProgramArguments</key>
    <array>
        <string>/Applications/MS SSO OpenConnect.app/Contents/Resources/venv/bin/python</string>
        <string>-m</string>
        <string>vpn_ui.privileged_helper</string>
    </array>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <true/>

    <key>StandardOutPath</key>
    <string>/var/log/ms-sso-openconnect-ui-helper.log</string>
    <key>StandardErrorPath</key>
    <string>/var/log/ms-sso-openconnect-ui-helper.error.log</string>
</dict>
</plist>
