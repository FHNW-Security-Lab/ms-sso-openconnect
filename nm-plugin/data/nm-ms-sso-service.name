[VPN Connection]
name=ms-sso
service=org.freedesktop.NetworkManager.ms-sso
program=/usr/libexec/nm-ms-sso-service
supports-multiple-connections=false

[libnm]
plugin=libnm-vpn-plugin-ms-sso-editor.so

[GNOME]
